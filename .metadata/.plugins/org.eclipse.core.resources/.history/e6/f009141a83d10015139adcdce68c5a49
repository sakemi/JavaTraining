package ch01.ex15;

public class TestLookupImpl {

	public static void main(String[] args) {
		LookupImpl lookup = new LookupImpl();

		lookup.add("foo", "hoge");
		lookup.add("bar", 123);

		assert((lookup.find("foo")).equals("hoge"));
		assert((Integer) lookup.find("bar") == 123);

		System.out.println("test ok");
	}
}
